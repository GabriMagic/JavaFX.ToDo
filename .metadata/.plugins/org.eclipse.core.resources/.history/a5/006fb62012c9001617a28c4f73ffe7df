package dad.todo.services;

import java.time.LocalDate;

import dad.todo.items.EventoItem;

public interface EventoService {

	public void crear(EventoItem evento);

	public void eliminar(Long id);

	public void actualizar(Long id);

	public EventoItem[] getEventos();

	public EventoItem[] buscarEventosPorFecha(LocalDate fecha);
}
